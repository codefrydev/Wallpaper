@page "/"
<PageTitle>Wallpaper</PageTitle>

<MudPaper Class="d-flex flex-column flex-grow-1 gap-2" Elevation="0">
    <MudPaper Width="100%" Height="49vh" @onclick="GenerativeSelected" />
    <MudPaper Width="100%" Height="49vh" @onclick="CreativeSelected"/> 
</MudPaper>

@code{
    [Inject] public NavigationManager NavigationManager { get; set; } = null!;
    void GenerativeSelected()
    {
        NavigationManager.NavigateTo("Generative");
    }
    void CreativeSelected()
    {
        NavigationManager.NavigateTo("Creative");
    }
}