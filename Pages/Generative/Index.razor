@page "/Generative"

<MudPaper 
    Class="d-flex flex-wrap justify-center"
    Elevation="0" 
    Style="background-color:#e5e9f4">
    @foreach (var item in generativeDraws)
    {
        <MudPaper Class="d-flex justify-center ma-2">
            <SKCanvasView 
                @ref="item.CanvasReference"
                OnPaintSurface="@item.OnPaintSurface" 
                @onclick="@item.ButtonClicked" id="@item.Id" />
        </MudPaper>
    }
</MudPaper>