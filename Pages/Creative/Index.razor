@page "/Creative"

<MudPaper Class="d-flex flex-wrap justify-center" Elevation="0">
    @foreach(var item in images)
    {
        <MudImage 
            Src="@item.ThumbNailUrl" 
            Width="200" 
            Height="150" 
            Alt="@item.Description" 
            Elevation="25" 
            Class="rounded-lg ma-2" 
            @onclick="@(()=>OpenDialog(item))" />
    }
</MudPaper>